<resources>

    <!-- Base application theme. -->
    <style name="AppTheme_Default" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <!--
       Base application theme, dependent on API level. This theme is replaced
       by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
   -->
    <style name="AppBaseTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:editTextBackground">@drawable/selector_edittext_bg</item>
    </style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <!-- 文本的基本格式 -->
    <style name="txt_base">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- 小字体 -->
    <style name="txt_small" parent="@style/txt_base">
        <item name="android:textSize">12sp</item>
    </style>

    <!-- 中字体 -->
    <style name="txt_middle" parent="@style/txt_base">
        <item name="android:textSize">14sp</item>
    </style>
    <!-- 大字体 -->
    <style name="txt_big" parent="@style/txt_base">
        <item name="android:textSize">16sp</item>
    </style>
    <!-- 超大字体 -->
    <style name="txt_large" parent="@style/txt_base">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="txt_white" parent="@style/txt_base">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="txt_gray" parent="@style/txt_base">
        <item name="android:textColor">#ff8d8d8d</item>
    </style>

    <style name="txt_red" parent="@style/txt_base">
        <item name="android:textColor">@color/red</item>
    </style>

    <!-- 水平分割线 -->
    <style name="divider_horizontal">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/dividing_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <!-- 水平分割线 -->
    <style name="divider_horizontal2">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/gray_ea</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <!-- 竖直分割线 -->
    <style name="divider_vertical">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/dividing_line</item>
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <!-- 竖直分割线 -->
    <style name="divider_vertical2">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/dividing_line2</item>
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <!-- ImageView的基本格式 -->
    <style name="img_base">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:contentDescription">@null</item>
    </style>

    <!-- 向导页面标示符 -->
    <style name="guide_dot" parent="@style/img_base">
        <item name="android:padding">15dp</item>
        <item name="android:src">@drawable/selector_img_dot</item>
    </style>

    <!-- LinearLayout的基本样式 -->
    <style name="ll_horizontal_base">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_vertical_base">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="horizontal_weight">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="vertical_weight">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="ll_horizontal_weight">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ll_horizontal_weight2">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_weight">2.0</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ll_vertical_weight">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:gravity">center</item>
    </style>

